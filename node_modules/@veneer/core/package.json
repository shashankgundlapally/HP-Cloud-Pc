    "ci:lint": "npm run ci:lint-styles && npm run ci:lint-scripts",
    "ci:lint-scripts": "(eslint src -o build/eslint.xml -f checkstyle || true) && eslint src && echo 'eslint: no lint errors'",
    "ci:lint-styles": "echo todo",
    "ci:package": "npm pack",
    "ci:publish": "npm publish",
    "ci:test": "jest --ci --coverage",
    "lint": "eslint src --quiet && echo \"eslint: no lint errors\"",
    "lint:ci": "eslint src -o eslint.xml -f checkstyle",
    "prebuild": "rimraf ./dist",
    "precommit": "npm run test:coverage && npm run lint",
    "prestart": "npm run build-styles",
    "pretest": "npm run build",
